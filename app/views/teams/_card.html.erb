<div class="col-xs-3 teammate-pic">
  <div class="member">
    <% if member.user.facebook_picture_url.nil? %>
      <% if member.user.profile_picture.present?  %>
        <%= cl_image_tag member.user.profile_picture, class: "members-picture", crop: :fill %>
      <% else %>
        <%= cl_image_tag("default_fkhyps.png", class: "members-picture") %>
      <% end %>
    <%else %>
      <%= image_tag member.user.facebook_picture_url, class: "members-picture", crop: :fill %>
    <% end %>
    <%= link_to "", user_path(member.user.id), class: "team-link" %>
  </div>
</div>




