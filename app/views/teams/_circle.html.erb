
<div class="circle">

  <% if member.user.facebook_picture_url.nil? %>
  <% unless member.user.profile_picture.nil?  %>
  <%= cl_image_tag member.user.profile_picture, class: "circle-large", crop: :fill %>
  <% end %>
  <%else %>
  <%= image_tag member.user.facebook_picture_url, class: "circle-large", crop: :fill %>
  <% end %>

  <%= member.user.first_name %> <%= member.user.last_name %>
  <%#= member.user.email %>
  <%= member.user.email %>
  <%= link_to "Delete", team_member_path(@team, member), method: :delete %>
  <%= link_to "Show", user_path(member.user.id) %>
  <%#= link_to "", user_path(member.user.id), class: "card-link" %>

</div>



